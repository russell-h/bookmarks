<ul>
  <% @bookmarks.each do |bookmark| %>
    <li>
    <a href="<%= bookmark.url %>">
    <%= bookmark.title %>
    </a>
    </li>
  <% end %>
</ul>

<form name="add-bookmarks" action="/add" method="post">
  <input type="text" name="url">
  <input type="text" name="title" placeholder="Title" />
  <input type="submit" value="Submit">


</form>